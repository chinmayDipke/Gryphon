package MuiltiThreading;

class Calculate extends Thread{
	public synchronized void run() {
		for(int i = 1; i<=10;i++) {
			System.out.println("5 x "+i+ "="+ (5*i));
		}	
	}
}

public class TablesThread extends Calculate{

	public static void main(String[] args) {
		Calculate op = new Calculate();
		
		Thread user1 = new Thread(op);
		Thread user2 = new Thread(op);
		
		user1.start();
		user2.start();

	}

}
