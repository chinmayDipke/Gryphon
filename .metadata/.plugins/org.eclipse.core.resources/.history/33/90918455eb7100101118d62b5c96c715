package MuiltiThreading;

class Amount{
	
	Amount(){
		
	}
}

public class BankThread extends Thread  {

	static int balance = 4000;
	
	public static void main(String[] args) {
		
		BankThread t1 = new BankThread();
		BankThread t2 = new BankThread();
		
		t1.setName("ABC");
		t2.setName("PQR");
		
		
		t1.start();
		t2.start();
		
	}

}
