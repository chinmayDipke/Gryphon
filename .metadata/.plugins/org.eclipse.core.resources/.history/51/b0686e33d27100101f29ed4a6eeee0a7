package MuiltiThreading;

class Calculate{
	
	for(int i = 1; i<=10;i++) {
		int table1 = 5;
		System.out.println(table1+" x "+i+ "= "+ (table1*i));
	}
}

class Calculate1 extends Calculate{
	
		for(int i = 1; i<=10;i++) {
			int table1 = 7;
			System.out.println(table1+" x "+i+ "= "+ (table1*i));
		}	

}

public class TablesThread extends Calculate1{
	
	
	TablesThread(int a) {
		super(a);
		// TODO Auto-generated constructor stub
	}

	public static void main(String[] args) {
		Calculate op = new Calculate();
		Calculate op1 = new Calculate();
		
		
		
		
	}

}
