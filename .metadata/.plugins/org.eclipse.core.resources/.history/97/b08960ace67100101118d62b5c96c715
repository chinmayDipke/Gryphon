package MuiltiThreading;

class PrintBook {
	static int ticket = 10;
	
	User() {
		System.out.println("Hello ");
	}
	synchronized(this) {
		System.out.println("Ticket Booked: "+(ticket-7));
		bye();
	}
	
	void bye() {
		System.out.println("Bye ");
	}
}

public class MultipleUser extends Thread {
	static PrintBook op;
	static int seat;
	public void run() {
		
	}
	public static void main(String[] args) {
		
		op = new PrintBook();
		
		MultipleUser t1 = new MultipleUser();
		MultipleUser t2 = new MultipleUser();
		
		t1.setName("ABC");
		t2.setName("PQR");
		t1.seat = 6;
		t2.seat = 6;

	}

}
