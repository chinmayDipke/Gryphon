		Stack<Integer> s2 = new Stack();
		Stack<Integer> s3 = new Stack();
		s2.push(23);
		s2.push(92);
		s2.push(98);
		s2.push(31);
		s2.push(3);
		s2.push(34);
		
		System.out.println("Stack s2: "+s2);
		System.out.println("Stack s3: "+s3);

		while(!s2.isEmpty()) 
		{
			int putval = 0;
			if(s3.isEmpty())
			{
				int temp=s2.pop();
				s3.push(temp);
			}
			else if(!s3.isEmpty())
		}
		
		System.out.println("=====================");
		System.out.println("Stack s3: "+s2);
		System.out.println("Stack s2: "+s3);

		
	}

}
